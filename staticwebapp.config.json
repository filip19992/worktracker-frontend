{
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; img-src 'self' blob: data:;"
    },
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/img/*.{png,jpg,gif,webp}", "/css/*"]
    },
    "mimeTypes": {
      "custom": "text/html"
    }
  }